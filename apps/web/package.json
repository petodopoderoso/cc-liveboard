{
  "name": "web",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy:dev": "opennextjs-cloudflare build && CLOUDFLARE_ENV=dev opennextjs-cloudflare deploy",
    "deploy:prod": "opennextjs-cloudflare build && CLOUDFLARE_ENV=prod opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "@opennextjs/cloudflare": "latest"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "typescript": "latest",
    "wrangler": "latest",
    "@tailwindcss/postcss": "latest",
    "tailwindcss": "latest"
  }
}
